#include<iostream>
using namespace std ;
int main(){
    int n ;
    cout<<"Enter the number of elements of array :" ; 
    cin>>n ;

    int arr[n];
    cout<<"Enter the elements of array : " ;
    for(int i = 0 ; i < n ; i++){
         cin>>arr[i] ;
    }

    int sum = 0 ;
    cout<<"Array is : " ;
    for(int i =0 ;i < n ; i++){
        cout<<arr[i]<<" ";
    }
    cout<<endl;
 
    cout<<"Sum of all elements of array is : " ;
    for(int i = 0 ;i < n ; i ++)
     sum +=arr[i];
     cout<<sum ;


}
